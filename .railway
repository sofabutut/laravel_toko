#!/bin/bash

# Set permission ke folder yang Laravel butuh tulis
chmod -R 775 storage
chmod -R 775 bootstrap/cache

# Jalankan cache dan migrate
php artisan config:cache
php artisan route:cache
php artisan view:cache

# Jalankan migrate (pastikan .env udah beres)
php artisan migrate --force

# Jalanin PHP server atau FPM (tergantung base image lo)
php-fpm
